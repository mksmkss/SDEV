{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":[
        "airbnb",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": [1, 200, 4],
        "no-alert": ["off"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx", ".ts", ".tsx"] }],
        "react/destructuring-assignment": [1]
        // "react/prop-types": "off"
    }
}
